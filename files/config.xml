<?xml version="1.0"?>
<zend-config xmlns:zf="http://framework.zend.com/xml/zend-config-xml/1.0/">
    <path>
        <pid>/var/run/searchd.pid</pid>
        <log>/var/log/sphinxsearch/searchd.log</log>
        <querylog>/var/log/sphinxsearch/query.log</querylog>
        <indexer>/usr/bin/indexer</indexer>
    </path>
    <indexer>
        <period>3600</period>
        <lastrun>0</lastrun>
        <runwithmaintenance></runwithmaintenance>
    </indexer>
    <maxresults>1000</maxresults>
</zend-config>
